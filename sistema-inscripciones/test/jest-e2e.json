{
  "preset": "@nestjs/testing",
  "testEnvironment": "node",
  "globalSetup": "./node_modules/@nestjs/testing/dist/global-setup.js",
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "src",
  "testRegex": ".spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "../coverage",
  "collectCoverageFrom": ["**/*.(t|j)s"]
}